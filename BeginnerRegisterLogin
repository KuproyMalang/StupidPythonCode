print("================Register=================")

x = input("Username : ")
y = input("Password : ")

print("===============Login=====================")

a = input("Username : ")
b = input("Password : ")

if b == y:
    if a == x:
        print("Welcome ^_^")
    else:
        print("Wrong!!!")
elif b != y:
    print("Wrong!!!")
else:
    print("Wrong!!!")
